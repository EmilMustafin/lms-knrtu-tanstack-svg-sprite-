{
  "name": "lms-knrtu",
  "author": {
    "name": "Emil Mustafin"
  },
  "description": "Template for LMS KNRTU",
  "keywords": [
    "vite",
    "react",
    "lms",
    "knrtu",
    "template"
  ],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "// DEV SCRIPTS": "----------------------------------",
    "dev": "vite",
    "dev:test": "start-server-and-test dev http://localhost:9001",
    "// BUILD SCRIPTS": "--------------------------------",
    "build": "tsc -b && vite build",
    "build:icons": "tsx ./src/shared/tools/build-icons.ts",
    "preview": "vite preview",
    "// TYPECHECKING AND LINTING": "---------------------",
    "lint": "eslint --report-unused-disable-directives --max-warnings 5 \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html,md,json}\"",
    "tsc:check": "tsc --build --noEmit",
    "tsc:watch": "tsc --build --noEmit --watch",
    "// UPDATE TOOLS": "-------------------------------",
    "update": "npx npm-check-updates -i",
    "// GIT AND COMMIT": "------------------------------",
    "prepare": "git init && husky || true",
    "commit:pre": "lint-staged --no-stash",
    "commit": "git-cz",
    "commit:devmoji": "devmoji -e --lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-router": "^1.97.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@feature-sliced/steiger-plugin": "^0.5.4",
    "@hookform/devtools": "^4.3.3",
    "@tanstack/eslint-plugin-query": "5",
    "@tanstack/eslint-plugin-router": "^1.97.0",
    "@tanstack/react-query-devtools": "^5.64.1",
    "@tanstack/router-devtools": "^1.97.3",
    "@tanstack/router-plugin": "^1.97.3",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cssnano": "^7.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "devmoji": "^2.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "node-html-parser": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "rollup-plugin-visualizer": "^5.14.0",
    "steiger": "^0.5.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-inspect": "^10.0.7"
  },
  "engines": {
    "node": "22",
    "pnpm": "9"
  },
  "packageManager": "pnpm@9.15.4",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --report-unused-disable-directives --max-warnings 5"
    ],
    "src/**/*.{css,scss,html,md,json}": [
      "prettier --write"
    ]
  }
}
